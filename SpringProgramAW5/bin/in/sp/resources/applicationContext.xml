<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

 
<bean class="in.sp.beans.Student" id="stdID" autowire="byType">
<property name="name" value="Alisha"></property>
<property name="roll" value="1"></property>

</bean>

<bean class="in.sp.beans.Address" id="addr1" autowire-candidate="false" >
<property name="houseno" value="111"></property>
<property name="city" value="Jamshedpur"></property>
<property name="pincode" value="832104"></property></bean>

<bean class="in.sp.beans.Address" id="addr2" >
<property name="houseno" value="121"></property>
<property name="city" value="Raipur"></property>
<property name="pincode" value="732114"></property></bean>
</beans>